{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appName": {
            "value": "sxx-az-wa-x-001"
        },
        "appType": {
            "value": "app"
        },
        "appServicePlanName": {
            "value": "sxx-az-wa-x-001-asp"
        },
        "appServicePlanSkuName": {
            "value": "P1v2"
        },
        "appServicePlanTier": {
            "value": "PremiumV2"
        },
        "appServicePlanSize": {
            "value": "P1v2"
        },
        "appServicePlanFamily": {
            "value": "Pv2"
        },
        "managedServiceIdentity": {
            "value": "SystemAssigned"
        },
        "siteConfig": {
            "value": {
                "metadata": [
                    {
                        "name": "CURRENT_STACK",
                        "value": "dotnetcore"
                    }
                ],
                "alwaysOn": true
            }
        },
        "httpsOnly": {
            "value": true
        },
        "enableMonitoring": {
            "value": true
        },
        "roleAssignments": {
            "value": [
                {
                    "roleDefinitionIdOrName": "Reader",
                    "principalIds": [
                        "<<deploymentSpId>>"
                    ]
                }
            ]
        }
    }
}
