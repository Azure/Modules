        "loadBalancingSettings": {
            "value": [
                {
                    "name": "loadBalancer",
                    "properties": {
                        "additionalLatencyMilliseconds": 0,
                        "sampleSize": 50,
                        "successfulSamplesRequired": 1
                    }
                }
            ]
        },
        "routingRules": {
            "value": [
                {
                    "name": "routingRule",
                    "properties": {
                        "acceptedProtocols": [
