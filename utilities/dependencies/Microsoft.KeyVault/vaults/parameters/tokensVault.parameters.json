  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "tokens-sxx-az-kv-x-001" //change this value when cloning
    },
    "accessPolicies": {
      "value": [
        {
          "tenantId": "449fbe1d-9c99-4509-9014-4fd5cf25b014", //to-do tokenize from GitHub
          "objectId": "<<deploymentSpId>>",
          "permissions": {
            "keys": [
